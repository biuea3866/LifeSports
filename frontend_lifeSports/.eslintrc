{
    "extends": "react-app",
    "plugins": ["prettier", "react"],
    "rules": {
      "prettier/prettier": "error"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "env":{
      "es6": true
    }
  }